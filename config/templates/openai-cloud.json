{
  "name": "openai-cloud",
  "description": "OpenAI embeddings with Qdrant Cloud",
  "embedding_provider": "openai",
  "vector_provider": "qdrant_cloud",
  "config_overrides": {
    "openai_model": "text-embedding-3-small",
    "openai_base_url": "https://api.openai.com/v1",
    "vector_size": 1536,
    "max_tokens": 8191
  },
  "required_env_vars": [
    "OPENAI_API_KEY",
    "QDRANT_CLOUD_URL",
    "QDRANT_API_KEY"
  ],
  "optional_env_vars": [
    "OPENAI_MODEL"
  ],
  "tags": [
    "openai",
    "cloud",
    "qdrant",
    "production"
  ]
}