{
  "name": "openai-local",
  "description": "OpenAI embeddings with local Qdrant",
  "embedding_provider": "openai",
  "vector_provider": "qdrant_local",
  "config_overrides": {
    "openai_model": "text-embedding-3-small",
    "openai_base_url": "https://api.openai.com/v1",
    "qdrant_url": "http://localhost:6333",
    "vector_size": 1536,
    "max_tokens": 8191
  },
  "required_env_vars": [
    "OPENAI_API_KEY"
  ],
  "optional_env_vars": [
    "OPENAI_MODEL",
    "QDRANT_URL"
  ],
  "tags": [
    "openai",
    "local",
    "qdrant"
  ]
}